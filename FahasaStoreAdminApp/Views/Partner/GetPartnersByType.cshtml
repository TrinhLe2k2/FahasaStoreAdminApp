@model IEnumerable<FahasaStoreAPI.Models.EntitiesModels.PartnerEntities>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PartnerId)
        </td>
        <td>
            @item.PartnerType?.Name
        </td>
        <td>
            <a href="/Partner/Details" IdValue="@item.PartnerId" onclick="HandlerCRUD(this, event)">@item.Name</a>
        </td>
        <td>
            <img src="@item.ImageUrl" alt="Image" width="50" height="50" class="object-fit-contain" />
        </td>
        <td>
            <a href="/Partner/Edit" IdValue="@item.PartnerId" onclick="HandlerCRUD(this, event)"><i class="bi bi-pencil"></i></a> |
            <a href="/Partner/Delete" IdValue="@item.PartnerId" onclick="HandlerCRUD(this, event)"><i class="bi bi-trash3"></i></a>
        </td>
    </tr>
}